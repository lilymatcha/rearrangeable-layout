{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/utils/react-beautiful-dnd/**/*.js",
      "src/utils/react-beautiful-dnd/**/*.jsx"
    ]
  },
  "rules": {
    "no-console": false,
    "jsx-no-lambda": false
  }
}
